                      span!=data.array[i].reserves.probable.amount
                    td.text-14.col-3
                      span!=data.array[i].reserves.probable.contents
                    td.text-14.col-4
                      span!=data.array[i].reserves.probable.aurum
                    td.text-14.col-5
                      span!=data.array[i].reserves.probable.ounces
