mixin resources(data)
  section.resources
    h2.visually-hidden="Ресурсы компании"
    ul.resources__places
      - for (let i = 0; i < data.array.length; i++)
        li.resources__item(data-toggle-item)
          .resources__place
            h3.heading-2.resources__title!=data.array[i].place
            button(type="button" data-toggle).resources__toggle
             +icon("icon-plus", 20, 20).resources__open
             +icon("icon-minus", 20, 20).resources__close
          table.table.table--desc
              tr.tr-flex
                th.heading-5.empty.col-0
                th.heading-5.col-1
                th.heading-5.col-2 Тонн, тыс. т
                th.heading-5.col-3 Содержание Au, г/т
                th.heading-5.col-4 Au, т
                th.heading-5.col-5 Au, тыс. унций
              tr.tr-flex
                td.heading-5.no-border.text.text--left.col-0 Ресурсы
                td.empty.col-1
                td.heading-5.col-2
                  span!=data.array[i].resources.total.amount
                td.heading-5.col-3
                  span!=data.array[i].resources.total.contents
                td.heading-5.col-4
                  span!=data.array[i].resources.total.aurum
                td.heading-5.col-5
                  span!=data.array[i].resources.total.ounces
              if data.array[i].resources.expected
                tr.tr-flex
                  if data.array[i].reserves
                    td.empty.col-0.no-border(rowspan="3")
                  else
                    td.col-0.no-border.empty(rowspan="3")
                  td.text-14.col-1.text.text--left.text--categories Предполагаемые
                  td.text-14.col-2
                    span!=data.array[i].resources.expected.amount
                  td.text-14.col-3
                    span!=data.array[i].resources.expected.contents
                  td.text-14.col-4
                    span!=data.array[i].resources.expected.aurum
                  td.text-14.col-5
                    span!=data.array[i].resources.expected.ounces
              if	data.array[i].resources.specified
                tr.tr-flex
                  td.empty.col-0.no-border(rowspan="3")
                  td.text-14.col-1.text.text--left.text--categories Указанные
                  td.text-14.col-2
                    span!=data.array[i].resources.specified.amount
                  td.text-14.col-3
                    span!=data.array[i].resources.specified.contents
                  td.text-14.col-4
                    span!=data.array[i].resources.specified.aurum
                  td.text-14.col-5
                    span!=data.array[i].resources.specified.ounces
              if	data.array[i].resources.measured
                tr.tr-flex
                  td.empty.col-0(rowspan="3")
                  td.text-14.text.text--left.text--categories.col-1 Измеренные
                  td.text-14.col-2
                    span!=data.array[i].resources.measured.amount
                  td.text-14.col-3
                    span!=data.array[i].resources.measured.contents
                  td.text-14.col-4
                    span!=data.array[i].resources.measured.aurum
                  td.text-14.col-5
                    span!=data.array[i].resources.measured.ounces
              if data.array[i].reserves
                tr.tr-flex
                  td.heading-5.no-border.text.text--left.col-0 Запасы
                  td.empty.col-1
                  td.heading-5.col-2
                    span!=data.array[i].reserves.total.amount
                  td.heading-5.col-3
                    span!=data.array[i].reserves.total.contents
                  td.heading-5.col-4
                    span!=data.array[i].reserves.total.aurum
                  td.heading-5.col-5
                    span!=data.array[i].reserves.total.ounces
                tr.tr-flex
                  td.no-border.empty.col-0(rowspan="2")
                  td.text-14.text.text--left.text--categories.col-1 Подтвержденные
                  td.text-14.col-2
